# API Keys
INOREADER_TOKEN=your_token_here

# Airtable Configuration
AIRTABLE_API_KEY=your_key_here
AIRTABLE_BASE_ID=appXXXXXX  # Base ID (starts with 'app')
AIRTABLE_TABLE_ID=tblXXXXXX  # Table ID (starts with 'tbl')
AIRTABLE_RATE_LIMIT=0.2  # Rate limit in seconds between requests
AIRTABLE_BATCH_SIZE=10   # Number of records to process in each batch

# Webhook Configuration
MAKE_WEBHOOK_URL=your_webhook_url_here

# Processing Configuration
RATE_LIMIT=0.2
MAX_RETRIES=3
LOG_LEVEL=INFO

# Content Validation
MAX_TITLE_LENGTH=255
MAX_CONTENT_LENGTH=2000
